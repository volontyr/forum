<h1>Log in</h1>
<%= form_for(:session, url: login_path) do |f| %>
  <p>
	<%= f.label :email %><br>
	<%= f.email_field :email %>
  </p>
  <p>	
	<%= f.label :password %><br>
	<%= f.password_field :password %>
  </p>
  <p><%= link_to 'Forgot password', new_password_reset_path %></p>
  <p>
	<%= f.submit 'Log in' %>
  </p>	
<% end %>
<p><%= link_to 'Sign up', signup_path %></p>	
